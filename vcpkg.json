{
  "name": "sqsgenerator",
  "version-string": "0.5.3",
  "features": {
    "cli": {
      "description": "Command line interface for SQS generator",
      "dependencies": [
        "eigen3",
        "boost-multiprecision",
        "nlohmann-json",
        "bshoshany-thread-pool",
        "argparse",
        "abseil"
      ]
    },
    "python": {
      "description": "Python bindings for SQS generator",
      "dependencies": [
        "eigen3",
        "boost-multiprecision",
        "nlohmann-json",
        "bshoshany-thread-pool",
        {
          "name": "abseil",
          "platform": "!windows"
        }
      ]
    },
    "tests": {
      "description": "Unit tests for SQS generator",
      "dependencies": [
        "eigen3",
        "boost-multiprecision",
        "nlohmann-json",
        "bshoshany-thread-pool",
        "gtest",
        "abseil"
      ]
    },
    "wasm": {
      "description": "WebAssembly support for SQS generator",
      "dependencies": [
        "eigen3",
        "boost-multiprecision",
        "nlohmann-json",
        "bshoshany-thread-pool"
      ]
    }
  }
}
